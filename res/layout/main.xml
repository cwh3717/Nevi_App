<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:weightSum="1"
              android:background="@drawable/main">

    <GridLayout
            android:layout_width="52dp"
            android:layout_height="81dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="24dp" android:layout_alignParentTop="true" android:layout_alignParentStart="true"
            android:id="@+id/A1">
    </GridLayout>
    <GridLayout android:layout_width="52dp" android:layout_height="81dp" android:layout_marginLeft="58dp"
                android:id="@+id/A2" android:layout_toEndOf="@+id/A1" android:layout_alignTop="@+id/A1"/>
    <GridLayout android:layout_width="94dp" android:layout_height="46dp" android:layout_marginLeft="101dp"
                android:id="@+id/B1" android:layout_toEndOf="@+id/A2" android:layout_alignTop="@+id/A2"/>
    <GridLayout android:layout_width="94dp" android:layout_height="46dp"
                android:id="@+id/B2" android:layout_toEndOf="@+id/A2"
                android:layout_below="@+id/B1" android:layout_alignStart="@+id/B1"/>
    <GridLayout android:layout_width="94dp" android:layout_height="46dp" android:id="@+id/B3"
                android:layout_toEndOf="@+id/A2" android:layout_below="@+id/B2" android:layout_alignStart="@+id/B2"
                android:layout_marginTop="1dp"/>
    <GridLayout android:layout_width="94dp" android:layout_height="46dp" android:id="@+id/B4"
                android:layout_toEndOf="@+id/A2" android:layout_below="@+id/B3" android:layout_alignStart="@+id/B3"
                android:layout_marginTop="1dp"/>
    <GridLayout android:layout_width="94dp" android:layout_height="46dp" android:id="@+id/B5"
                android:layout_toEndOf="@+id/A2"
                android:layout_below="@+id/B4" android:layout_alignStart="@+id/B4"
                android:layout_marginTop="1dp"/>
    <GridLayout android:layout_width="94dp" android:layout_height="46dp" android:id="@+id/B6"
                android:layout_toEndOf="@+id/A2"
                android:layout_below="@+id/B5" android:layout_alignStart="@+id/B5"
                android:layout_marginTop="1dp"/>
    <GridLayout android:layout_width="94dp" android:layout_height="46dp" android:id="@+id/B7"
                android:layout_toEndOf="@+id/A2"
                android:layout_below="@+id/B6" android:layout_alignStart="@+id/B6" android:layout_marginTop="1dp"/>
    <GridLayout android:layout_width="94dp" android:layout_height="46dp" android:id="@+id/B8"
                android:layout_toEndOf="@+id/A2"
                android:layout_below="@+id/B7" android:layout_alignStart="@+id/B7" android:layout_marginTop="1dp"/>
    <GridLayout android:layout_width="94dp" android:layout_height="46dp" android:id="@+id/B9"
                android:layout_toEndOf="@+id/A2"
                android:layout_below="@+id/B8" android:layout_alignStart="@+id/B8" android:layout_marginTop="1dp"
                android:focusableInTouchMode="false"/>
    <GridLayout android:layout_width="94dp" android:layout_height="46dp" android:id="@+id/gridLayout5"
                android:layout_toEndOf="@+id/A2"
                android:focusableInTouchMode="false" android:layout_below="@+id/B9"
                android:layout_alignStart="@+id/B9" android:layout_marginTop="1dp"/>
</RelativeLayout>

